{
  "manifest_version": 3,
  "name": "IVS",
  "description": "Increase Video Speed",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "rush.png"
  },
  "content_scripts": [
    {
      "js": ["./scripts/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "storage", "tabs", "scripting"
  ],
  "host_permissions": ["https://*/*"]
}
